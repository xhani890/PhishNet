{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "==== ORIGINAL SCRIPTS ====": "",
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:win": "set NODE_ENV=development&& tsx server/index.ts",
    "dev:unix": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production tsx server/index.ts",
    "start:win": "set NODE_ENV=production&& node dist/index.js",
    "start:unix": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit push",
    "import-data": "tsx scripts/import-data.ts",
    "setup": "npm run db:push && npm run import-data",
    "kali": "NODE_ENV=development tsx server/index.ts",
    
    "==== ZERO TRUST SECURITY ====": "",
    "security:scan": "npm audit --audit-level=high",
    "security:secrets": "echo 'Scanning for secrets...' && grep -r 'password\\|secret\\|key\\|token' . --exclude-dir=node_modules || echo 'No secrets found'",
    "security:multi-tenancy": "echo 'Testing multi-tenancy isolation...'",
    
    "==== ACCESS CONTROL ====": "",
    "access:verify": "bash .github/scripts/verify-access.sh",
    "access:check-permissions": "echo 'Checking user permissions...'",
    
    "==== TESTING WORKFLOWS ====": "",
    "test": "jest",
    "test:unit": "jest --testPathPattern='tests/unit' --coverage",
    "test:integration": "jest --testPathPattern='tests/integration'",
    "test:e2e": "playwright test",
    "test:security": "npm run security:multi-tenancy",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov > coverage/lcov.info",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    
    "==== MODULAR ACCESS WORKFLOWS ====": "",
    "setup:frontend": "echo 'Setting up frontend workspace...' && cp .env.frontend .env && npm install",
    "setup:backend": "echo 'Setting up backend workspace...' && cp .env.codespace .env && npm install && npm run db:migrate",
    "setup:database": "echo 'Setting up database workspace...' && cp .env.codespace .env && npm run db:migrate",
    
    "frontend:dev": "vite --host 0.0.0.0 --port 5173",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview --host 0.0.0.0 --port 4173",
    "frontend:mock": "node scripts/frontend-mock-server.js",
    "frontend:full": "concurrently \"npm run frontend:mock\" \"npm run frontend:dev\"",
    "frontend:check": "cd client && tsc --noEmit",
    "frontend:check:watch": "cd client && tsc --noEmit --watch",
    "backend:dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "app:preview": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "app:full": "npm run dev",
    
    "==== WORKSPACE VALIDATION ====": "",
    "validate:frontend": "node scripts/validate-workspace.js frontend",
    "validate:backend": "node scripts/validate-workspace.js backend", 
    "validate:database": "node scripts/validate-workspace.js database",
    
    "==== ZERO TRUST SETUP ====": "",
    "setup:zero-trust": "bash scripts/setup-zero-trust.sh",
    "ci:security": "npm run security:scan && npm run access:verify"
  },
